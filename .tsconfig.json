{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"jsx": "preserve",
		"noEmit": true,
		"isolatedModules": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"typeRoots": [],
		"forceConsistentCasingInFileNames": true,
		"exclude": ["**/node_modules/**/*"]
	}
}
